    "def solution(s):\n",
    "    lenStr = len(s)\n",
    "    if lenStr % 2 == 0:\n",
    "        return [s[i:i+2] for i in range(0, lenStr, 2)]\n",
    "    else:\n",
    "        temp = [s[i:i+2] for i in range(0, lenStr, 2)]\n",
    "        temp[-1] = temp[-1] + \"_\"\n",
    "        return temp\n",
    "\n",
    "solution(\"abc\")"
   ]
